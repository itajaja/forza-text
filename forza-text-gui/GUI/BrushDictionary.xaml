<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<!-- Resource dictionary entries should be defined here. -->
	<Color x:Key="ControlBackgroundColor">#2E1700</Color>
	<SolidColorBrush x:Key="ControlBackgroundBrush" Color="#FF333333" />
	
	<Color x:Key="ControlBorderColor">#FF1F1F1F</Color>
	<SolidColorBrush x:Key="ControlBorderBrush" Color="{DynamicResource ControlBorderColor}" />
	
	<Color x:Key="ControlBackGroundColorMouseOver">#2672EC</Color>
	<Color x:Key="ControlBackGroundColorPressed">#2E8DEF</Color>
	
	<Color x:Key="ControlContentColor">#FFB2B2B2</Color>
	<SolidColorBrush x:Key="ControlContentBrush" Color="{DynamicResource ControlContentColor}" />
</ResourceDictionary>
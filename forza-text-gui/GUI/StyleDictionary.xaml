<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<Style TargetType="TextBlock" x:Key="TextBlockStyle">
		<Setter Property="FontFamily" Value="Ubuntu Light"/>
		<Setter Property="FontSize" Value="13.33"/>
		<Setter Property="Foreground" Value="{DynamicResource ControlContentBrush}"/>
	</Style>
	<Style TargetType="Button">
		<Setter Property="Template" Value="{DynamicResource ButtonControlTemplate1}"/>
	</Style>
</ResourceDictionary>
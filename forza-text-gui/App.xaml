<Application x:Class="forza_text_gui.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="GUI/MainWindow.xaml">
    <Application.Resources>
         
    	<ResourceDictionary>
    		<ResourceDictionary.MergedDictionaries>
    			<ResourceDictionary Source="GUI/TemplateDictionary.xaml"/>
    			<ResourceDictionary Source="GUI/BrushDictionary.xaml"/>
    			<ResourceDictionary Source="GUI/StyleDictionary.xaml"/>
    			<ResourceDictionary Source="GUI/StringDictionary.xaml"/>
    		</ResourceDictionary.MergedDictionaries>
    	</ResourceDictionary>
         
    </Application.Resources>
</Application>
